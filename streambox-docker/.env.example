# Environment Variables for Docker Compose
# Copy this file to .env and update the values

# Restreamer
RS_USERNAME=admin
RS_PASSWORD=changeme

# Grafana
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=changeme

# LLDAP 
LLDAP_JWT_SECRET=REPLACE_WITH_RANDOM_32_CHAR_STRING
LLDAP_KEY_SEED=REPLACE_WITH_RANDOM_32_CHAR_STRING
LLDAP_LDAP_BASE_DN=dc=streaming,dc=local
LLDAP_LDAP_USER_PASS=changeme

# Timezone
TZ=America/New_York

# User/Group IDs (use 'id' command to get your values)
UID=1000
GID=1000